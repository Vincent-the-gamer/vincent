{
  "name": "vincent",
  "type": "module",
  "version": "1.1.1",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "build": "cross-env NODE_ENV=production vite-ssg build && tsx ./scripts/copy-fonts.ts && tsx ./scripts/rss.ts && pnpm run copy-files",
    "spa-build": "cross-env NODE_ENV=production vite build && tsx ./scripts/copy-fonts.ts && tsx ./scripts/rss.ts && pnpm run copy-files",
    "copy-files": "cp _dist_redirects dist/_redirects",
    "dev": "vite --open",
    "preview": "vite preview",
    "redirects": "tsx scripts/redirects.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dep": "taze major -I"
  },
  "dependencies": {
    "@snackbar/core": "^1.7.0",
    "@vueuse/core": "^13.4.0",
    "aplayer": "^1.10.1",
    "dayjs": "^1.11.13",
    "floating-vue": "^5.2.2",
    "nprogress": "^0.2.0",
    "pixi.js": "^8.10.2",
    "sharp": "^0.34.2",
    "simplex-noise": "^4.0.3",
    "typed.js": "^2.1.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-router-better-scroller": "^0.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@iconify/json": "^2.2.353",
    "@octokit/rest": "^22.0.0",
    "@shikijs/markdown-it": "^3.7.0",
    "@shikijs/transformers": "^3.7.0",
    "@shikijs/twoslash": "^3.7.0",
    "@types/diacritics": "^1.3.3",
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/nprogress": "^0.2.3",
    "@types/three": "^0.177.0",
    "@unhead/vue": "^2.0.11",
    "@unocss/preset-web-fonts": "66.3.2",
    "@unocss/reset": "^66.3.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "cross-env": "^7.0.3",
    "diacritics": "^1.3.0",
    "eslint": "^9.30.0",
    "eslint-plugin-format": "^1.0.1",
    "fast-glob": "^3.3.3",
    "feed": "^5.1.0",
    "fs-extra": "^11.3.0",
    "gray-matter": "^4.0.3",
    "lint-staged": "^16.1.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-github-alerts": "^1.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-table-of-contents": "^0.9.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "shiki": "^3.7.0",
    "simple-git-hooks": "^2.13.0",
    "taze": "^19.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unocss": "^66.3.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue-markdown": "^28.3.1",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^7.0.0",
    "vite-plugin-inspect": "^11.2.0",
