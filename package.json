    "nprogress": "^0.2.0",
    "sharp": "^0.33.5",
    "three": "^0.175.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-router-better-scroller": "^0.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
