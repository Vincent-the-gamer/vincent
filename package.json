  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "simple-git-hooks",
      "unrs-resolver"
    ]
  },
  "resolutions": {
    "vite": "^7.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
